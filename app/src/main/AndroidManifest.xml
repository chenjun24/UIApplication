<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cj.uiapplication">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UIApplication">
        <activity android:name=".view.layoutmanager.LayoutManagerActivity"></activity>
        <activity android:name=".view.annotation.AnnotationActivity" />
        <activity android:name=".view.testfragment.TestFragmentActivity" />
        <activity android:name=".view.handler.HandlerActivity" />
        <activity
            android:name=".view.layout.LayoutActivity"
            android:label="layout" />
        <activity android:name=".view.measure.MeasureActivity" />
        <activity
            android:name=".view.draw.DrawActivity"
            android:label="draw" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>